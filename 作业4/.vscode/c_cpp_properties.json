{
  "configurations": [
    {
      "name": "linux-gcc-x64",
      "includePath": [
        "${workspaceFolder}/**",
        "/usr/local/include/ceres",
        "/usr/local/include/ceres/internal",
        "/usr/local/lib",
        "/usr/include/eigen3",
        "/usr/include/glog",
        "/usr/include/gflags",
        "/usr/lib/x86_64-linux-gnu"
      ],
      "compilerPath": "/usr/bin/gcc",
      "cStandard": "${default}",
      "cppStandard": "${default}",
      "intelliSenseMode": "linux-gcc-x64",
      "compilerArgs": [
        "-Wall",
        "-Wextra",
        "-Wpedantic"
      ]
    }
  ],
  "version": 4
}